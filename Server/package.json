    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.22",
    "@types/mongodb": "^3.3.8",
    "@types/node": "^12.11.7",
    "@types/socket.io": "^2.1.4",
    "eslint": "^6.3.0",
    "jest": "^24.9.0",
    "jshint": "^2.10.2",
    "nodemon": "^1.19.4",
